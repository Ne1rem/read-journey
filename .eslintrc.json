{
    "extends": [
        "next",
        "next/core-web-vitals",
        "plugin:react-hooks/recommended",
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:sonarjs/recommended",
        "prettier"
    ],
    "plugins": ["prettier", "sonarjs", "@typescript-eslint"],
    "parser": "@typescript-eslint/parser",
    "rules": {
        "prettier/prettier": [
            "warn",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
